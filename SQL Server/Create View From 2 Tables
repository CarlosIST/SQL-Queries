CREATE VIEW [MONTHLY_SAMPLING_DATA_TABLEAU] AS 
SELECT * 
	FROM [EXSALES_OPS].[dbo].[MONTHLY_SAMPLING_CAR_DATA]

	LEFT JOIN [EXSALES_OPS].[dbo].[MONTHLY_SAMPLING_CALLS_DATA] 
	ON [EXSALES_OPS].[dbo].[MONTHLY_SAMPLING_CAR_DATA].[DATE_KEY] = [EXELTIS_SALES_OPS].[dbo].[MONTHLY_SAMPLING_CALLS_DATA].[DATE_KEY_CALLS]

	LEFT JOIN [EXELTIS_SALES_OPS].[dbo].[MONTHLY_SAMPLING_GOGOMEDS_DATA]
	ON [EXSALES_OPS].[dbo].[MONTHLY_SAMPLING_CAR_DATA].[DATE_KEY] = [EXELTIS_SALES_OPS].[dbo].[MONTHLY_SAMPLING_GOGOMEDS_DATA].[DATE_KEY_GOGO]
	
