SELECT YEAR([CALL_WEEK]) AS "YEAR_GOGOMEDS", 
	   MONTH([CALL_WEEK]) AS "MONTH_GOGOMEDS", 
	   COUNT (*) AS "SAMPLE RXS", 
	   CONCAT(YEAR([CALL_WEEK]), '-', MONTH([CALL_WEEK])) AS "DATE_KEY_GOGO"

INTO [EXSALES_OPS].[dbo].[MONTHLY_SAMPLING_GOGOMEDS_DATA]
FROM [EXSALES_OPS].[dbo].[GOGOMEDS_OUTPUT]
GROUP BY YEAR([CALL_WEEK]), MONTH([CALL_WEEK]) --FORMAT([Shipped Date], 'MMM', 'en-US')
ORDER BY YEAR([CALL_WEEK]), MONTH([CALL_WEEK])

GO
